material MyMaterials/wood1
{
    technique
    {
        pass
        {
            scene blend alpha_blend
            // depth_write off
            
            texture_unit
            {
                texture wood1.png
                scale 1 1
                alpha_op_ex source1 src_manual src_current 0.5
            }
        }   
    }
}

